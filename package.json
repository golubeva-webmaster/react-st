    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,html,css}": "prettier --write",
    "src/**/*.{ts,tsx}": "eslint --color"
